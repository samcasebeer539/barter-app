{
  "expo": {
    "name": "Barter App",
    "slug": "barter-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barterapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.barterapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {},
    "scheme": "barterapp",
    "plugins": [
      [
        "expo-build-properties", 
        {
            "ios":{
                "useFrameworks": "static"
            }
        }
    ]
    ]
  }
}
